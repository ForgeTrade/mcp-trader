# Binance API Credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here
BINANCE_BASE_URL=https://api.binance.com

# Traefik Configuration
# Email for Let's Encrypt notifications
ACME_EMAIL=admin@context8.markets

# Traefik Dashboard Basic Auth
# Username: admin
# Password: Generated secure password
# To generate new: docker run --rm httpd:alpine htpasswd -nb admin your_password
# IMPORTANT: Escape $ symbols as $$ in .env file for docker-compose
TRAEFIK_BASIC_AUTH=admin:$$apr1$$oh7s8Exg$$5By06ax6Ke6Mho6pi2w5T1

# Traefik Log Level (DEBUG, INFO, WARN, ERROR)
TRAEFIK_LOG_LEVEL=INFO

# Rust Logging
RUST_LOG=info
