# Binance Provider Configuration
# Copy this file to .env and fill in your credentials

# Binance API Credentials (required for authenticated endpoints)
# Get your API keys from: https://www.binance.com/en/my/settings/api-management
BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Binance Base URL (switch between testnet and production)
# Production: https://api.binance.com
# Testnet: https://testnet.binance.vision
BINANCE_BASE_URL=https://testnet.binance.vision

# Logging Level (optional)
# Options: error, warn, info, debug, trace
RUST_LOG=info

# gRPC Server Port (optional, default: 50053)
# Note: Ensure this matches the port in mcp-gateway/providers.yaml
# GRPC_PORT=50053
